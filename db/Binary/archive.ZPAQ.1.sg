// Author: LinXP
// Status: WIP

meta("archive", "ZPAQ Compressed Archive (.ZPAQ)");

function detect() {
    if (Binary.getSize() >= 9) {
        if (Binary.compare("'7kSt'") && Binary.compare("'zPQ'", 0x0d)) {
            bDetected = true;
        }
    }

    return result();
}
