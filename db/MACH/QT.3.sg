// Detect It Easy: detection rule file

meta("library", "QT");

function getVersion() {
    var current = MACH.getLibraryCurrentVersion("QtCore");

    return ((current >> 16) & 0xff) + "." + ((current >> 8) & 0xff) + "." + (current & 0xff);
}

function detect() {
    if (MACH.isLibraryPresent("QtCore")) {
        sVersion = getVersion();
        bDetected = true;
    }

    return result();
}
